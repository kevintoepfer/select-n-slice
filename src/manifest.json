{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Simple Slice",
      "description": "Create a slice around your selection.",
      "icon": "icons\/simpleslice.png",
      "identifier": "simpleslice",
      "script": "./simpleslice.js"
    },
    {
      "name": "Padding Slice",
      "description": "Create a slice with a set padding around your selection.",
      "icon": "icons\/paddingslice.png",
      "identifier": "paddingslice",
      "script": "./paddingslice.js"
    }
  ],
  "menu": {
    "title": "Select 'n' Slice",
    "items": [ "simpleslice", "paddingslice" ]
  }
}